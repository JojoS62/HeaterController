{
    "target_overrides": {
        "*": {
            "target.printf_lib": "std", 
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-buffered-serial": 1,
            "mbed-http.http-buffer-size" : 1024,
            "platform.all-stats-enabled": 0,
            "mbed-trace.enable": 0,
            "lwip.socket-max": 10,
            "lwip.tcp-socket-max": 10                  
        },
        "DISCO_H747I": {
            "target.network-default-interface-type": "ETHERNET",
            "target.macros_add": [
                "DISCO_H747I_ETHERNET_SOLDERBRIGE"
            ],
            "device_has_add": ["SDIO", "EMAC"],
            "device_has_remove": ["QSPIF"]
        }
    }
}